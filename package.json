{
  "name": "astro-tailwind-sanity-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "Production-ready starter template with Astro 5, Tailwind CSS v4, and Sanity CMS",
  "license": "MIT",
  "keywords": [
    "astro",
    "tailwind",
    "sanity",
    "cms",
    "starter",
    "boilerplate",
    "template",
    "typescript"
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"src/**/*.{astro,ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{astro,ts,tsx,js,jsx,json,css,md}\"",
    "check": "astro check",
    "clean": "rm -rf dist .astro node_modules/.astro node_modules/.vite"
  },
  "dependencies": {
    "@astro-community/astro-embed-vimeo": "^0.3.12",
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.6.1",
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.14.0",
    "@sanity/image-url": "^2.0.2",
    "@sanity/visual-editing": "^5.0.4",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.8",
    "astro-favicons": "^3.1.5",
    "astro-icon": "^1.1.5",
    "astro-portabletext": "^0.13.0",
    "astro-seo": "^0.8.4",
    "blaze-slider": "^1.9.3",
    "clsx": "^2.1.1",
    "lucide-astro": "^0.556.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2"
  }
}
